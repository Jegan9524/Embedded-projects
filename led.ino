int g=5;
int r=6;
int y=7;
void setup()
{
  pinMode(g,OUTPUT);
  pinMode(r,OUTPUT);
  pinMode(y,OUTPUT);
}
void loop()
{
  for(int i=1;i==1;i++)
  {
    digitalWrite(g,HIGH);
    delay(1000);
    digitalWrite(g,LOW);
    delay(1000);
  }
   for(int i=1;i<=2;i++)
  {
    digitalWrite(g,HIGH);
    delay(1000);
    digitalWrite(g,LOW);
    delay(1000);
  }
   for(int i=1;i<=3;i++)
  {
    digitalWrite(g,HIGH);
    delay(1000);
    digitalWrite(g,LOW);
    delay(1000);
  }
}
